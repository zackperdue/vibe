# Test variable assignments without type annotations
a = 2
b = 3
c = a + b
print(c)  # Should output 5

# Test closure with assignments
def makeAdder(x: int): function do
  def inner(y: int): int do
    return x + y
  end
  return inner
end

add5 = makeAdder(5)
result = add5(10)
print(result)  # Should output 15